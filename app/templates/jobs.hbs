{{page-title "Results"}}

<JobFilter @results={{@model}}>
  <Pagination
    @id="pagination-section"
    @page={{this.page}}
    @pages={{this.pages}}
    @changePage={{this.changePage}}
  />
</JobFilter>

<div id="job-list">
  {{#each this.jobs as |job|}}
    <Job::Card @job={{job}} />
  {{/each}}
</div>

<Pagination
  @id="pagination-footer"
  @page={{this.page}}
  @pages={{this.pages}}
  @changePage={{this.changePage}}
/>